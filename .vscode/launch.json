{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch via ts-node",
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "args": [
          "${workspaceFolder}/src/main.ts"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  